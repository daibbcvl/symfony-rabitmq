<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Blog Admin - {% block title %}{% endblock %}</title>
    <link rel="shortcut icon" href="{{ asset('images/favicon.ico') }}" type="image/x-icon">

    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
            google: {"families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"]},
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>

    <!--end::Web font -->
    {% block stylesheets %}
        {{ encore_entry_link_tags('admin', null, 'adminConfig') }}
    {% endblock %}
</head>
<body class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--fixed m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default {% block body_class %}{% endblock %}">
{% block body %}
    <!-- begin:: Page -->
    <div class="m-grid m-grid--hor m-grid--root m-page">
        <!-- BEGIN: Header -->
        {% include 'admin.header.html.twig' %}
        <!-- END: Header -->

        <!-- begin::Body -->
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">

            <!-- BEGIN: Left Aside -->
            <button class="m-aside-left-close  m-aside-left-close--skin-dark " id="m_aside_left_close_btn"><i
                        class="la la-close"></i></button>
            <div id="m_aside_left" class="m-grid__item	m-aside-left  m-aside-left--skin-dark ">

                <!-- BEGIN: Aside Menu -->
                <div id="m_ver_menu" class="m-aside-menu  m-aside-menu--skin-dark m-aside-menu--submenu-skin-dark "
                     m-menu-vertical="1" m-menu-scrollable="1" m-menu-dropdown-timeout="500"
                     style="position: relative;">
                    <ul class="m-menu__nav  m-menu__nav--dropdown-submenu-arrow ">
                        <li class="m-menu__section ">
                            <h4 class="m-menu__section-text">Components</h4>
                            <i class="m-menu__section-icon flaticon-more-v2"></i>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu"><a href="{{ path('category_index') }}"
                                                                           class="m-menu__link"><i
                                        class="m-menu__link-icon la la-building"></i><span class="m-menu__link-text">Category</span></a>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu"><a href="{{ path('tag_index') }}"
                                                                           class="m-menu__link"><i
                                        class="m-menu__link-icon la la-tags"></i><span class="m-menu__link-text">Tag</span></a>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu"><a href="{{ path('post_index') }}"
                                                                           class="m-menu__link"><i
                                        class="m-menu__link-icon la la-sticky-note"></i><span class="m-menu__link-text">Post</span></a>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu"><a href="{{ path('comment_index') }}"
                                                                           class="m-menu__link"><i
                                        class="m-menu__link-icon la la-comment"></i><span class="m-menu__link-text">Comment</span></a>
                        <li class="m-menu__item  m-menu__item--submenu"><a href="{{ path('page_index') }}"
                                                                           class="m-menu__link"><i
                                        class="m-menu__link-icon la la-sticky-note"></i><span class="m-menu__link-text">Page</span></a>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu"><a href="{{ path('country_index') }}"
                                                                           class="m-menu__link"><i
                                        class="m-menu__link-icon la fa-globe"></i><span class="m-menu__link-text">Country</span></a>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu"><a href="{{ path('destination_index') }}"
                                                                           class="m-menu__link"><i
                                        class="m-menu__link-icon la la-map-marker"></i><span class="m-menu__link-text">Destination</span></a>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu"><a href="{{ path('user_index') }}"
                                                                           class="m-menu__link"><i
                                        class="m-menu__link-icon la flaticon-user"></i><span class="m-menu__link-text">User</span></a>
                        </li>
                    </ul>
                </div>

                <!-- END: Aside Menu -->
            </div>

            <!-- END: Left Aside -->
            {% block body_wrapper %}
                <div class="m-grid__item m-grid__item--fluid m-wrapper">

                    <!-- BEGIN: Subheader -->
                    {% block subheader %}
                        <div class="m-subheader ">
                            <div class="d-flex align-items-center">
                                <div class="mr-auto">
                                    <h3 class="m-subheader__title m-subheader__title--separator">{% block subheader_title %}{% endblock %}</h3>
                                    {% block subheader_breadcrumbs %}
                                        <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
                                            <li class="m-nav__item m-nav__item--home">
                                                <a href="/" class="m-nav__link m-nav__link--icon">
                                                    <i class="m-nav__link-icon la la-home m--margin-left-5"></i>
                                                </a>
                                            </li>
                                            {% if breadcrumbs is defined %}
                                                {% for item in breadcrumbs %}
                                                    <li class="m-nav__separator">-</li>
                                                    <li class="m-nav__item">
                                                        {% if item.url is defined %}
                                                            <a href="{{ item.url }}" class="m-nav__link">
                                                                <span class="m-nav__link-text">{{ item.text }}</span>
                                                            </a>
                                                        {% else %}
                                                            <span class="m-nav__link-text">{{ item.text }}</span>
                                                        {% endif %}
                                                    </li>
                                                {% endfor %}
                                            {% endif %}
                                        </ul>
                                    {% endblock %}
                                </div>
                                {% block subheader_actions %}
                                    {% if quick_actions is defined %}
                                        <div class="m-dropdown m-dropdown--inline m-dropdown--arrow m-dropdown--align-right m-dropdown--align-push"
                                             m-dropdown-toggle="hover" aria-expanded="true">
                                            <a href="#"
                                               class="m-portlet__nav-link btn btn-lg btn-secondary  m-btn m-btn--outline-2x m-btn--air m-btn--icon m-btn--icon-only m-btn--pill  m-dropdown__toggle">
                                                <i class="la la-plus m--hide"></i>
                                                <i class="la la-ellipsis-h"></i>
                                            </a>
                                            <div class="m-dropdown__wrapper">
                                                <span class="m-dropdown__arrow m-dropdown__arrow--right m-dropdown__arrow--adjust"></span>
                                                <div class="m-dropdown__inner">
                                                    <div class="m-dropdown__body">
                                                        <div class="m-dropdown__content">
                                                            <ul class="m-nav">
                                                                <li class="m-nav__section m-nav__section--first m--hide">
                                                                    <span class="m-nav__section-text">Quick Actions</span>
                                                                </li>
                                                                {% for item in quick_actions %}
                                                                    <li class="m-nav__item">
                                                                        {% if item.url is defined %}
                                                                            <a href="{{ item.url }}"
                                                                               class="m-nav__link">
                                                                                <i class="m-nav__link-icon {{ item.icon }}"></i>
                                                                                <span class="m-nav__link-text">{{ item.text }}</span>
                                                                            </a>
                                                                        {% else %}
                                                                            <div class="m-nav__link">
                                                                                <i class="m-nav__link-icon {{ item.icon }}"></i>
                                                                                <span class="m-nav__link-text">{{ item.text }}</span>
                                                                            </div>
                                                                        {% endif %}
                                                                    </li>
                                                                {% endfor %}
                                                                {% block quick_actions_items %}{% endblock %}
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    {% endif %}
                                {% endblock %}
                            </div>
                        </div>
                    {% endblock %}

                    <!-- END: Subheader -->
                    {% block content %}{% endblock %}
                </div>
            {% endblock %}
        </div>

        <!-- end:: Body -->
    </div>

    <!-- end:: Page -->

    <!-- begin::Quick Sidebar -->
    {% block quick_sidebar %}
        <div id="m_quick_sidebar" class="m-quick-sidebar m-quick-sidebar--tabbed m-quick-sidebar--skin-light">
            <div class="m-quick-sidebar__content m--hide">
                <span id="m_quick_sidebar_close" class="m-quick-sidebar__close"><i class="la la-close"></i></span>
                {% block quick_sidebar_content %}{% endblock %}
            </div>
        </div>
    {% endblock %}

    <!-- end::Quick Sidebar -->

    <!-- begin::Scroll Top -->
    <div id="m_scroll_top" class="m-scroll-top">
        <i class="la la-arrow-up"></i>
    </div>

    <!-- end::Scroll Top -->

    <!-- begin::Flash messages -->
    {% for label, messages in app.flashes(['success', 'error', 'warning']) %}
        {% for message in messages %}
            <div class="flash-message m--hide" data-flash-type="{{ label }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
    <!-- end::Flash messages -->
{% endblock %}
{% block javascripts %}
    {{ encore_entry_script_tags('admin', null, 'adminConfig') }}
{% endblock %}
</body>
</html>
